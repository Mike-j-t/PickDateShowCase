<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="pickdate_cell">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:text">23</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/pickdatecell</item>
        <item name="android:onClick">clickDateCell</item>
    </style>
    <style name="pickdate_daterow">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="pickdate_dategridcolumnheader">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textStyle">bold|italic</item>
    </style>
    <style name="pickdate_adjustbutton">
        <item name="android:layout_width">0dp</item>
        <item name="android:padding">1dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/black</item>
    </style>
</resources>